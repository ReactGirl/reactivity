{
  "setupFiles": ["<rootDir>/src/setupTest.js"],
  "collectCoverageFrom": ["src/**/*.{js,jsx}", "!src/**/*.test.{js,jsx}"],
  "coverageThreshold": {
    "global": {
      "statements": 98,
      "branches": 91,
      "functions": 98,
      "lines": 98
    }
  },
  "moduleDirectories": ["node_modules", "src"],
  "moduleNameMapper": {
    "^components(.*)$": "<rootDir>/src/components$1",
    "^reducers(.*)$": "<rootDir>/src/redux/reducers$1",
    "^.+\\.(css|scss)$": "identity-obj-proxy",
    "\\.(gif|ttf|eot|svg)$": "<rootDir>/fileTransformer.js"
  },
  "testRegex": "tests/.*\\.test\\.js$"
}
