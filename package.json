  "scripts": {
    "start:server": "webpack --config webpack.config.server.js --watch --hot",
    "start:client": "webpack-dev-server --config webpack.config.client.js",
    "start:process": "node bin/start-server.watch.js",
    "start": "rm -rf dist && npm-run-all --parallel start:server start:client start:process",
    "production:client": "webpack --config webpack.config.client.js -p --colors --display-error-details",
    "production:server": "webpack --config webpack.config.server.js -p --colors --display-error-details",
    "production": "rm -rf dist; npm-run-all --parallel production:server production:client; node bin/start-server.js",
    "generate:favicon": "real-favicon generate favicon.config.json .favicon static",
    "lint": "eslint -c .eslintrc src",
    "newStart": "NODE_ENV=development babel-watch server/index.js"
