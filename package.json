    "start": "rm -rf dist && npm-run-all --parallel start:server start:client start:process",
    "production:client": "webpack --config webpack.config.client.js -p --colors --display-error-details",
    "production:server": "webpack --config webpack.config.server.js -p --colors --display-error-details",
